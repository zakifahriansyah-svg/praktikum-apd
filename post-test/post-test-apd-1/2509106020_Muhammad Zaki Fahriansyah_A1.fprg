    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Nilai" type="Integer" array="False" size=""/>
            <output expression="&quot;Masukkan Nilai&quot;" newline="True"/>
            <input variable="Nilai"/>
            <if expression="Nilai &gt;= 80">
                <then>
                    <output expression="&quot;Nilai A&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="Nilai &gt;= 70 and Nilai &lt;= 79">
                        <then>
                            <output expression="&quot;Nilai B&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="Nilai &gt;= 60 and Nilai &lt;= 69">
                                <then>
                                    <output expression="&quot;Nilai C&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Nilai D&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
